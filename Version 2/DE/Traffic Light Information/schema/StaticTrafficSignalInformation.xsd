      <xs:element name="trafficStream" type="D2LogicalModel:TrafficStream" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Traffic Streams (in driving direction) are encountered anticlockwise, beginning from the most southern starting point, i.e. a left turning stream would appear before a straight stream.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="id" type="xs:string" use="required" />
    <xs:attribute name="version" type="xs:string" use="required" />
  </xs:complexType>
  <xs:complexType name="StopLinePoint">
    <xs:annotation>
      <xs:documentation>A connection point between a StopLine and a TrafficStream with some georeference indication. One main signalGroup and optional a sub signalGroup are assoiciated to this point.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="xOffsetToTrafficStream" type="D2LogicalModel:MetresAsFloat" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>x offset from startpoint of traffic stream to StopLinePoint</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="yOffsetToTrafficStream" type="D2LogicalModel:MetresAsFloat" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>y offset from startpoint of traffic stream to StopLinePoint</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="percentageDistanceAlong" type="D2LogicalModel:Percentage" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>A measure of distance along a linear element from the start of the element expressed as a percentage of the total length of the linear object.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="stopLineBearing" type="D2LogicalModel:AngleInDegrees" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Bearing of the vertical of the stop line (i.e. driving direction; 360 degree from North)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="lanePositionOnRoadSegment" type="D2LogicalModel:NonNegativeInteger" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Indicates the position of the right-most lane of the stop line point in correlation to the road segment (from right to left in driving direction). Example: numberOfLanes = 2 and lanePos.. = 3 means that lane 3 and 4 are belonging to the stop line point.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="numberOfLanes" type="D2LogicalModel:NonNegativeInteger" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The number of lanes on some road or for some traffic stream. If omited, the numberOfLanes is unkown.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="mainSignalGroupId" type="D2LogicalModel:String" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifier of the main signal group. Signal groups are no DATEX components, but the IDs are reused in the dynamic model.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="subSignalGroupId" type="D2LogicalModel:String" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>If there is a sub signal group available, it can be referenced here. Signal groups are no DATEX components, but the IDs are reused in the dynamic model.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="trafficSignalId" type="D2LogicalModel:String" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifier of the traffic signal (e.g. FA1). Traffic signals are not DATEX components, the ID has no further effect within this model.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="turnAllowedWithoutSignal" type="D2LogicalModel:Boolean" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>It is allowed to turn right or left (depending on traffic stream) any time (observing priority rules)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="pointCoordinates" type="D2LogicalModel:PointCoordinates" minOccurs="0" />
      <xs:element name="stopLinePointExtension" type="D2LogicalModel:_ExtensionType" minOccurs="0" />
    </xs:sequence>
    <xs:attribute name="id" type="xs:string" use="required" />
  </xs:complexType>
  <xs:complexType name="StopLinePointByReference">
    <xs:annotation>
      <xs:documentation>Reference to an existing stop line point in case the corresponding traffic signal groups are identical.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="referenceToStopLinePoint" type="D2LogicalModel:_StopLinePointReference" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Reference to an existing stop line point.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="xOffsetToTrafficStreamOverride" type="D2LogicalModel:MetresAsFloat" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Overrides the x-offset information from the original stop line point.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="yOffsetToTrafficStreamOverride" type="D2LogicalModel:MetresAsFloat" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Overrides the y-offset information from the original stop line point.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="percentageDistanceAlongOverride" type="D2LogicalModel:Percentage" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Overrides the percentagedistanceAlongt information from the original stop line point.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="pointCoordinates" type="D2LogicalModel:PointCoordinates" minOccurs="0" />
      <xs:element name="stopLinePointByReferenceExtension" type="D2LogicalModel:_ExtensionType" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="String">
    <xs:annotation>
      <xs:documentation>A character string whose value space is the set of finite-length sequences of characters. Every character has a corresponding Universal Character Set code point (as defined in ISO/IEC 10646), which is an integer.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="1024" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="TrafficStream">
    <xs:annotation>
      <xs:documentation>A driving connection (usually within an intersection) with references to one or more StopLinePoints.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="stopLinePoint" type="D2LogicalModel:StopLinePoint" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="stopLinePointByReference" type="D2LogicalModel:StopLinePointByReference" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="linear" type="D2LogicalModel:Linear" />
      <xs:element name="trafficStreamExtension" type="D2LogicalModel:_ExtensionType" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
</xs:schema>
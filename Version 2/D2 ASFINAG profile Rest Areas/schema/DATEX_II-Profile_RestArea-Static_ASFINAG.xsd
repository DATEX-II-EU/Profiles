      <xs:element name="vmsOperator" type="D2LogicalModel:Contact" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="parkingVMSExtension" type="D2LogicalModel:_ExtensionType" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>

        <xs:annotation>
          <xs:documentation>Number of vehicles or devices, which can be charged simultaneously (sum over all electric charing stations specified with the 'numberOf...' attribute). If omitted, 1 charging point per station is assumed.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="electricChargingExtension" type="D2LogicalModel:_ExtensionType" minOccurs="0" />
    </xs:sequence>

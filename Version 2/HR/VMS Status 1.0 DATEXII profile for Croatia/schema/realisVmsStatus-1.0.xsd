      <xs:extension base="D2LogicalModel:VmsSetting">
        <xs:sequence>
          <xs:element name="vmsUnitTableReference" type="D2LogicalModel:_VmsUnitTableVersionedReference" minOccurs="1" maxOccurs="1">

          <xs:documentation>Less than 10 parking spaces available.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="lessThan20SpacesAvailable">
        <xs:annotation>
          <xs:documentation>Less than 20 parking spaces available.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
